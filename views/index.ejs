                        <input type="text" class="form-control" id="inputEpigrafe" placeholder="Epigrafe" ng-model="post.epigrafe">
                    </div>
                    <br>
                    <div class="form-input">
                        Descripcion
                        <textarea class="form-control" id="inputDescripcion" rows="4" placeholder="Descripcion" ng-model="post.descripcion"></textarea>
                    </div>
                    <br>
                    <div class="form-input">
                        Inicio
                        <input type="date" class="form-control" id="inputInicio"  min="2014-10-01" max="2015-12-31" ng-model="post.inicio">
                    </div>
                    <br>
                    <div class="form-input">
                        Duracion
                        <input type="text" class="form-control" id="inputDuracion" placeholder="Duracion" ng-model="post.duracion">
                    </div>
                    <br>
                    <div class="form-input">
                        Hora Inicio
                        <input type="text" class="form-control" id="horainicio" placeholder="Hora Inicio" ng-model="post.horainicio">
                    </div>
                    <br>
                    <div class="form-input">
                        Hora Fin
                        <input type="text" class="form-control" id="horafin" placeholder="Hora Fin" ng-model="post.horafin">
                    </div>
                    <br>
                    <div class="form-input">
                        Valor
                        <input type="number" class="form-control" id="valor" ng-model="post.valor">
                    </div>
                    <br>
                    <div class="form-input">
                        Calle
                        <input type="text" class="form-control" id="calle" ng-model="post.calle">
                    </div>
                    <br>
                    <div class="form-input">
                        Numero
                        <input type="number" class="form-control" id="numero" ng-model="post.numero">
                    </div>
                    <br>
                    <div class="form-input">
                        Departamento
                        <select id="sel" class="form-control" ng-model="post.departamento"
                                ng-options="departamento.nombre as departamento.nombre for departamento in departamentos.data">
                        </select>
                    </div>
                    <div class="form-group">
                        Seleccione una Imagen
                        <input type="file" name="file" id="file" sube-archivo="post.foto" style="border:2px solid #F0FFFF;border-radius:20px;box-shadow:2px 2px #808080, -2px -2px #D3D3D3;color:#3A87AD;" />
                        <ul class="thumbnails" data-ng-show="post.foto.toString() != '[object Object]' && post.foto.toString() != ''" data-ng-cloak>
                            <li class="span4">
                                <div class="thumbnail" style="display:table;height:75px;margin-top:5px;">
                                    <img ng-src="{{post.foto}}" alt="" style="height:75px;border:1px solid #000;display: block;"/>
                                </div>
                            </li>
                        </ul>
                    </div>

                    <br>
                    <br>
                    <div class="form-input">
                        Ultimos Cupos
                        <input type="checkbox" class="form-control" id="ultimos_cupos" ng-model="post.ultimos_cupos">
                    </div>
                    <br>
                    <div class="form-input col-md-offset-5">
                        <button type="submit" class="btn btn-primary">Editar</button>
                    </div>
                </form>

            </div>


        </div>
    </script>

